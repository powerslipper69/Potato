#include <stdio.h>
char stack[100]; int top = -1;

void push(char c) { stack[++top] = c; }
char pop() { return stack[top--]; }
int match(char o, char c) {
    return (o=='('&&c==')')||(o=='{'&&c=='}')||(o=='['&&c==']');
}

int main() {
    char exp[100]; int i, valid = 1;
    printf("Enter expression: ");
    scanf("%s", exp);
    for (i = 0; exp[i]; i++) {
        char c = exp[i];
        if (c=='('||c=='{'||c=='[') push(c);
        else if (c==')'||c=='}'||c==']') {
            if (top==-1 || !match(pop(), c)) { valid = 0; break; }
        }
    }
    if (valid && top==-1) printf("Parentheses Balanced\n");
    else printf("Parentheses Not Balanced\n");
}
